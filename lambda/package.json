{
    "name": "lambda",
    "private": true,
    "version": "1.0.0",
    "description": "Lambda function",
    "type": "commonjs",
    "main": "index.js",
    "scripts": {
        "prepare": "husky",
        "clean": "find . -name 'dist' -type d -prune -exec rm -rf '{}' + && find . -name '*.tsbuildinfo' -type f -delete",
        "build": "yarn clean && tsc",
        "test": "vitest --config ./vitest.config.ts",
        "coverage": "vitest run --coverage",
        "package": "mkdir -p bundle && rsync -a . bundle/ --exclude bundle --exclude lambda-code.zip --exclude .git --exclude node_modules && cd bundle && yarn install --production --frozen-lockfile && zip -r ../lambda-code.zip . && cd .. && rm -rf bundle",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write \"**/*.ts\""
    },
    "dependencies": {
        "@aws-sdk/client-secrets-manager": "^3.529.1",
        "@supabase/supabase-js": "^2.45.1",
        "@types/winston": "^2.4.4",
        "axios": "^1.8.4",
        "dotenv": "^16.4.5",
        "openai": "^5.6.0",
        "winston": "^3.17.0",
        "zod": "^3.25.67"
    },
    "devDependencies": {
        "@types/nock": "^11.1.0",
        "@types/node": "^20.14.12",
        "@typescript-eslint/eslint-plugin": "^8.34.1",
        "@typescript-eslint/parser": "^8.34.1",
        "aws-sdk-client-mock": "^4.1.0",
        "c8": "^10.1.3",
        "eslint": "^9.29.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-prettier": "^5.5.0",
        "husky": "^9.1.7",
        "nock": "^14.0.5",
        "prettier": "^3.5.3",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.5.4",
        "typescript-eslint": "^8.34.1",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.2.3"
    }
}
